{
  "parser": "babel-eslint",
  "extends": [ "airbnb" ],
  "plugins": [ "import", "react"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "linebreak-style": 0,
    "comma-dangle": "off",
    "eol-last": 0,
    "react/prefer-stateless-function": "off",
    "react/forbid-prop-types": 0,
    "max-len": [
      "error", {
      "ignorePattern": "\\s*<",
      "ignoreUrls": true,
      "code": 100
    }],
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".js", ".jsx"] }
    ],
    "jsx-a11y/anchor-is-valid": [
      "error",
      {
        "components": [ "Link" ],
        "specialLink": [ "to" ]
      }
    ],
    "no-use-before-define": [
      "error",
      { "functions": false }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}